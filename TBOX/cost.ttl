ci:Activity
  rdf:type owl:Class ;
  rdfs:label "Activity" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom cterm:CategoryOfActivity ;
      owl:onProperty cterm:hasCategory ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom cterm:FamilyOfActivity ;
      owl:onProperty cterm:hasFamily ;
    ] ;
  rdfs:subClassOf [
