cr:Assignment
  rdf:type owl:Class ;
  rdfs:label "Assignment"@en ;
  rdfs:label "Relazione"@it ;
  rdfs:label "Zuweisung"@de ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty cr:isAssignmentOf ;
      owl:someValuesFrom cr:AssignmentSet ;
    ] ;
.
cr:AssignmentSet
  rdf:type owl:Class ;
  rdfs:label "Assignment set" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty cr:hasAssignment ;
      owl:someValuesFrom cr:Assignment ;
    ] ;
.
cr:Consumption
  rdf:type owl:Class ;
  rdfs:label "Consumo"@it ;
  rdfs:label "Consumption"@en ;
  rdfs:label "Verbrauch"@de ;
